        <activity android:name="org.techtown.AndroidStudioAigoyak.SubMedicineSearch"/>
        <activity android:name="org.techtown.AndroidStudioAigoyak.B_AddMedicineSearchList"/>
        <activity android:name="org.techtown.AndroidStudioAigoyak.D_SearchListShape"/>







